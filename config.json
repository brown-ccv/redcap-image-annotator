{
  "name": "Image Annotator",

  "namespace":"BrownCCV\\ImageAnnotator",

  "description": "Annotate uploaded images on a form/survey",

  "framework-version": 3,

  "authors": [
    {
      "name": "Matthew Slivinski",
      "email": "matthew_slivinski@brown.edu",
      "institution": "Brown University"
    },
    {
      "name": "Mary McGrath",
      "email": "mary_mcgrath@brown.edu",
      "institution": "Brown University"
    }
  ],

  "permissions": [
    "hook_data_entry_form_top",
    "hook_survey_page_top",
    "hook_every_page_top",
    "hook_every_page_before_render"
  ],

  "links": {
    "control-center": [
    ],
    "project": [
    ]
  },

  "system-settings": [
    {
      "key": "log-path",
      "name": "<b>OPTIONAL</b><br>Leave blank for no logging.<br>Enter a valid server logging path to enable module logging (e.g. <pre>/var/log/image_annotator.log</pre>)<br>If the log path is not valid, it will log to the php_error log",
      "required": false,
      "type": "text"
    }
  ],

  "project-settings": [
    {
      "key": "javascript-debug",
      "name": "Output debug information in the browser console",
      "type": "checkbox"
    }
  ],
  "compatibility": {
      "php-version-min": "5.6.0",
      "php-version-max": "",
      "redcap-version-min": "9.7.8",
      "redcap-version-max": ""
   }
}
